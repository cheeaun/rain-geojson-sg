{
  "name": "rain-geojson-sg",
  "version": "1.0.0",
  "description": "Straight-forward API server to convert rain area radar images (Singapore) to GeoJSON",
  "main": "index.js",
  "engines": {
    "node": ">8.0.0"
  },
  "dependencies": {
    "@turf/area": "~6.0.1",
    "@turf/helpers": "~6.1.4",
    "@turf/intersect": "~6.1.3",
    "feed": "~4.1.0",
    "geojson-rewind": "~0.3.1",
    "got": "~10.2.1",
    "micro": "~9.3.4",
    "micro-cors": "~0.1.1",
    "nearest-color": "~0.4.4",
    "now-micro": "~0.4.0",
    "pngjs": "~3.4.0",
    "polygon-clipping": "~0.14.3",
    "rgb-hex": "~3.0.0"
  },
  "devDependencies": {
    "micro-dev": "~3.0.0"
  },
  "scripts": {
    "start": "micro -l tcp://0.0.0.0:${PORT-3000}",
    "dev": "micro-dev -i README.md"
  },
  "keywords": [
    "rain",
    "singapore",
    "weather",
    "temperature",
    "humidity"
  ],
  "author": "Lim Chee Aun",
  "license": "MIT"
}
