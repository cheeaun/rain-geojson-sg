{
  "name": "rain-geojson-sg",
  "version": "1.0.0",
  "description": "Straight-forward API server to convert rain area radar images (Singapore) to GeoJSON",
  "main": "index.js",
  "dependencies": {
    "@turf/helpers": "~6.1.4",
    "geojson-rewind": "^0.3.1",
    "got": "~8.3.1",
    "micro": "~9.3.1",
    "micro-cors": "~0.1.0",
    "nearest-color": "^0.4.2",
    "pngjs": "~3.3.3",
    "polygon-clipping": "~0.6.1",
    "rgb-hex": "~2.1.0"
  },
  "devDependencies": {
    "micro-dev": "~3.0.0"
  },
  "scripts": {
    "start": "micro -p ${PORT-3000}",
    "dev": "micro-dev -i README.md"
  },
  "keywords": [
    "rain",
    "singapore",
    "weather",
    "temperature",
    "humidity"
  ],
  "author": "Lim Chee Aun",
  "license": "MIT",
  "now": {
    "alias": [
      "rain-geojson-sg.now.sh",
      "api.checkweather.sg"
    ]
  }
}
